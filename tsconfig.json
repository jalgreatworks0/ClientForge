{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false,
    "resolveJsonModule": true,
    "moduleResolution": "node10",
    "allowJs": true,
    "strict": false,
    "noEmitOnError": false,
    "ignoreDeprecations": "6.0"
  },
  "files": []
}
